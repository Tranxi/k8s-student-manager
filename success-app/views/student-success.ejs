<!-- main page of student -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Success</title>
</head>
<body>
    <h1>Student Success Page</h1>
    <h2>Your Grades</h2>
    <ul>
        <% grades.forEach(function(grade) { %>
            <li><%= grade.courseName %>: <%= grade.grade %></li>
        <% }) %>
    </ul>
</body>
</html>


